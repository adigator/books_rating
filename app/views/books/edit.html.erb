<%= form_for @book, html: { multipart: true } do |f| %>
    <%= f.label :image %>
    <%= f.file_field :image %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :year %>
    <%= f.text_field :year %>
    <%= f.label :publisher %>
    <%= f.text_field :publisher %>
    <%= f.label :genre %>
    <%= f.text_field :genre %>
    <%= f.label :description %>
    <%= f.text_field :description %>
    <%= f.label :country %>
    <%= f.text_field :country %>
    <%= f.submit "Edytuj",class:"btn-submit" %>
<% end %>


