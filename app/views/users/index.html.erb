<div class="container view user-index mt30">

  <% @users.each do |user| %>
      <div class="row">
        <div class="col-xs-12 col-sm-12 col-md-8 col-md-offset-2 col-lg-6 col-lg-offset-3">
          <div class="col-xs-3 "><%= user.id %></div><div class="col-xs-9 b-s-a text-left"><%= link_to user.username, user_path(user) %></div>
        </div>
      </div>
      

  <% end %>
</div>